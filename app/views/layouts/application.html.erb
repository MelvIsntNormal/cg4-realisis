        <!--Render the app header-->
		<%= render 'shared/header' %>
        <!--Render the sidebar-->
		<%= render 'shared/aside' %>
        <!--Declare the main division-->
		<div id="main">
            <!--Declare an inner division to house description-->
			<div class="buffer">
              <!--Outputs string provided by page-->
				<%= yield(:pagetitle) %>
